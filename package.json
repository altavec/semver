{
  "name": "semver-action",
  "version": "0.0.0",
  "private": true,
  "description": "GitHub action for Altavec.SemanticVersioning",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "pack": "ncc build",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run pack && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/altavec/semver.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "Altavec",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.9.1",
    "@actions/exec": "^1.1.0"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/node": "^16.11.7",
    "@typescript-eslint/parser": "^5.10.0",
    "@vercel/ncc": "^0.33.1",
    "eslint-plugin-github": "^4.3.5",
    "eslint-plugin-jest": "^25.7.0",
    "js-yaml": "^4.1.0",
    "prettier": "2.5.1",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.4"
  }
}
